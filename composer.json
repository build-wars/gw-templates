{
	"$schema": "https://getcomposer.org/schema.json",
	"name": "buildwars/gw-templates",
	"description": "A Guild Wars build template encoder/decoder.",
	"license": "MIT",
	"type": "library",
	"keywords": ["guild wars"],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		},
		{
			"name": "Contributors",
			"homepage":"https://github.com/build-wars/gw-templates/graphs/contributors"
		}
	],
	"funding": [
		{
			"type": "Ko-Fi",
			"url": "https://ko-fi.com/codemasher"
		}
	],
	"homepage": "https://github.com/build-wars/gw-templates",
	"support": {
		"issues": "https://github.com/build-wars/gw-templates/issues",
		"source": "https://github.com/build-wars/gw-templates"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": "^8.1",
		"ext-sodium": "*"
	},
	"require-dev": {
		"phan/phan": "^5.4",
		"phpunit/phpunit": "^10.5",
		"phpmd/phpmd": "^2.15",
		"squizlabs/php_codesniffer": "^3.10"
	},
	"suggest": {
	},
	"autoload": {
		"psr-4": {
			"BuildWars\\GWTemplates\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BuildWars\\GWTemplatesTest\\": "tests"
		}
	},
	"scripts": {
		"phan": "@php vendor/bin/phan",
		"phpcs": "@php vendor/bin/phpcs",
		"phpunit": "@php vendor/bin/phpunit"
	},
	"config": {
		"lock": false,
		"sort-packages": true,
		"platform-check": true
	}
}
